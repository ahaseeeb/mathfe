{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "All Gifted": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "math",
            "index": "src/index.html",
            "main": "src/main.ts",
            "tsConfig": "src/tsconfig.app.json",
            "polyfills": "src/polyfills.ts",
            "assets": [
              "src/assets",
              "src/favicon.ico",
              "src/images",
              "src/videos",
              "src/.htaccess"
            ],
            "styles": [
              "src/css/bootstrap.min.css",
              "src/styles.css",
              "src/css/animate.min.css",
              "src/css/nivo-lightbox.css",
              "src/css/owl.carousel.css",
              "src/css/owl.theme.css",
              "src/assets/elegant-icons/style.css",
              "src/css/responsive.css",
              "src/css/styles.css"
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.min.js",
              "src/js/bootstrap.min.js",
              "src/js/jquery.fitvids.js",
              "src/js/jquery.localScroll.min.js",
              "src/js/jquery.stellar.min.js",
              "src/js/jquery.nav.js",
              "src/js/jquery.scrollTo.min.js",
              "src/js/nivo-lightbox.min.js",
              "src/js/owl.carousel.min.js",
              "src/js/respond.min.js",
              "src/js/retina.min.js",
              "src/js/simple-expand.min.js",
              "src/js/smoothscroll.js",
              "src/js/wow.min.js",
              "src/js/jquery.ajaxchimp.js",
              "src/js/custom.js",
              "src/js/charts.js"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "All Gifted:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "All Gifted:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "All Gifted:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "scripts": [
              "src/js/bootstrap.min.js",
              "src/js/jquery.fitvids.js",
              "src/js/jquery.localScroll.min.js",
              "src/js/jquery.stellar.min.js",
              "src/js/jquery.nav.js",
              "src/js/jquery.scrollTo.min.js",
              "src/js/nivo-lightbox.min.js",
              "src/js/owl.carousel.min.js",
              "src/js/respond.min.js",
              "src/js/retina.min.js",
              "src/js/simple-expand.min.js",
              "src/js/smoothscroll.js",
              "src/js/wow.min.js",
              "src/js/custom.js",
              "src/js/charts.js"
            ],
            "styles": [
              "src/css/bootstrap.min.css",
              "src/styles.css",
              "src/css/animate.min.css",
              "src/css/nivo-lightbox.css",
              "src/css/owl.carousel.css",
              "src/css/owl.theme.css",
              "src/assets/elegant-icons/style.css",
              "src/css/responsive.css",
              "src/css/styles.css"
            ],
            "assets": [
              "src/assets",
              "src/favicon.ico",
              "src/images",
              "src/videos"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": []
          }
        }
      }
    },
    "All Gifted-e2e": {
      "root": "",
      "sourceRoot": "",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./protractor.conf.js",
            "devServerTarget": "All Gifted:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "e2e/tsconfig.e2e.json"
            ],
            "exclude": []
          }
        }
      }
    }
  },
  "defaultProject": "All Gifted",
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "ag",
      "styleext": "css"
    },
    "@schematics/angular:directive": {
      "prefix": "ag"
    }
  }
}